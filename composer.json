{
    "name": "yutawhee/sparda-cms-core-module",
    "type": "laravel-module",
    "description": "",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "5.7.*",
        "nwidart/laravel-modules": "^4.1",
        "joshbrw/laravel-module-installer": "^0.1.4",
        "laravelcollective/html": "^5.7",
        "intervention/image": "^2.5",
        "alexusmai/laravel-file-manager": "^2.0",
        "doctrine/dbal": "^2.0",
        "unisharp/laravel-filemanager": "^1.9"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gdevilbat\\SpardaCMS\\Modules\\Core\\Providers\\CoreServiceProvider"
            ],
            "aliases": {

            }
        }
    },
    "autoload": {
        "psr-4": {
            "Gdevilbat\\SpardaCMS\\Modules\\Core\\": ""
        },
        "exclude-from-classmap": [
            "/preprocessor/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
          "Gdevilbat\\SpardaCMS\\Modules\\Core\\Tests\\": "Tests"
        }
    },
    "archive": {
        "exclude": ["/Tests", "/preprocessor"]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
